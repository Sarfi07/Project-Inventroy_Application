extends layout 

block content 

    h1.page-title #{title}

    form(method='post', enctype='multipart/form-data')
        .form-group 
            label(for='file') Upload an Image: 
            input#file.form-control(type='file', name='file', required)
        
        .form-group
            button.btn.btn-primary(type='submit') Submit